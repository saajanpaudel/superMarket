@model List<SuperMarket.Models.employeeModel>

<table class="table table-stripped" >
    <tr>
        <th>Employee ID</th>
        <th>Name</th>
        <th>Address</th>
        <th>Salary</th>
        <th>Employee level</th>
        <th>Photo</th>
        <th>Action</th>
    </tr>
    @foreach(var employee in Model)
    {
        <tr>
            <td>@employee.emp_id</td>
            <td>@employee.name</td>
            <td>@employee.address</td>
            <td>@employee.salary</td>
            <td>@employee.levels</td>
            <td>@employee.photo</td>
            <td><a asp-controller="Employee" asp-action="Edit" asp-route-id="@employee.emp_id"><i class="fas fa-edit fa-2x"></i></a> | <a asp-controller="Employee" asp-action="Delete" asp-route-id="@employee.emp_id" onclick="return confirm('Are you sure you want to delete?')"><i class="fas fa-trash-alt fa-2x red"></i></a> | <a asp-controller="Employee" asp-action="Details" asp-route-id="@employee.emp_id"><i class="far fa-eye fa-2x green"></i></a></td>
        </tr>
    }
</table>